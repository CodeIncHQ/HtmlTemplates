{
  "name": "codeinc/html-templates",
  "version": "3.0.0-dev",
  "description": "A collection of HTML templates",
  "homepage": "https://github.com/CodeIncHQ/HtmlTemplates",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "codeinc/error-renderer": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {"CodeInc\\HtmlTemplates\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\HtmlTemplates\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "suggest": {
    "codeinc/error-renderer": "A lib to render exceptions and errors into HTML or CLI"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
